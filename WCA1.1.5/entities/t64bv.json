        "value": 1
      },
      "minecraft:pushable": {
        "is_pushable": false,
        "is_pushable_by_piston": true
      },
      "minecraft:rideable": {
        "crouching_skip_interact": true,
        "family_types": ["player"],
        "interact_text": "Into turret",
