        "value": 0.9
      },
      "minecraft:type_family": {
        "family": ["entity", "ussr", "ussrtank", "t64bv", "tank"]
      },
      "minecraft:underwater_movement": {
        "value": 0.02
      }
    },
    "description": {
      "animations": {
        "engine": "animation.tank_engine",
        "firing": "controller.animation.t64bv_shoot",
        "glow": "controller.animation.tank_shoot_glow",
        "smoking": "controller.animation.tank",
        "tank": "controller.animation.tank_speed"
      },
      "identifier": "ussr:t64bv",
      "is_experimental": false,
      "is_spawnable": true,
      "is_summonable": true,
      "scripts": {
        "animate": [
          "firing",
          { "engine": "q.has_rider" },
          "glow",
          "tank",
          "smoking"
        ]
      }
    },
    "events": {
      "maingun:shoot_end": {
        "add": {
          "component_groups": ["maingun:ready"]
        },
        "remove": {
          "component_groups": ["maingun:shoot_start"]
        }
      },
      "maingun:shoot_start": {
        "add": {
          "component_groups": ["maingun:shoot_start"]
        },
        "remove": {
          "component_groups": ["maingun:shoot_end"]
        }
      },
      "minecraft:dead": {
        "add": {
          "component_groups": ["minecraft:is_dead"]
        }
      },
      "minecraft:entity_spawned": {
        "add": {
          "component_groups": ["maingun:ready", "machinegun:ready"]
        },
        "remove": {
          "component_groups": ["maingun:shoot_start", "machinegun:shoot_start"]
        }
      }
    }
  }
}