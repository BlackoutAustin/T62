        "interactions": [
          {
            "cooldown": 6,
            "interact_text": "Shoot Maingun",
            "on_interact": {
              "event": "maingun:shoot_start",
              "filters": {
                "all_of": [
                  {
                    "domain": "hand",
                    "operator": "equals",
                    "subject": "player",
                    "test": "has_equipment",
                    "value": "flint_and_steel"
                  },
                  {
                    "subject": "other",
                    "test": "is_riding",
                    "value": true
                  },
                  {
                    "operator": "equals",
                    "subject": "self",
                    "test": "is_variant",
                    "value": 0
                  }
                ]
              },
              "target": "self"
            },
            "spawn_entities": "ussr:t64bv_shell"
          }
        ]
      },
      "minecraft:is_stackable": {},
      "minecraft:knockback_resistance": {
        "value": 1
      },
      "minecraft:movement": {
        "value": 0.1
